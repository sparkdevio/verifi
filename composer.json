{
  "name": "meness/verifi",
  "description": "A Laravel package to handle email verification.",
  "keywords": [
    "laravel",
    "email",
    "verification"
  ],
  "homepage": "https://github.com/meness/verifi",
  "license": "MIT",
  "time": "2017-05-30",
  "authors": [
    {
      "name": "Alireza Eskandarpour Shoferi",
      "email": "aesshoferi@gmail.com",
      "homepage": "https://about.me/meness",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "aesshoferi@gmail.com",
    "issues": "https://github.com/meness/verifi/issues",
    "source": "https://github.com/meness/verifi",
    "docs": "https://github.com/meness/verifi/blob/master/README.md"
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "illuminate/contracts": "~5.4",
    "illuminate/queue": "~5.4",
    "illuminate/auth": "~5.4",
    "illuminate/bus": "~5.4",
    "illuminate/notifications": "~5.4",
    "illuminate/support": "~5.4",
    "illuminate/database": "~5.4",
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Meness\\Verifi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
